{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "mhm/link-404",
    "version": "1.0.0",
    "title": "Archive link",
    "category": "widgets",
    "icon": "admin-links",
    "description": "Provides an alternative link to the blog archive site.",
    "supports": {
        "html": false,
        "color": {
            "background": true,
            "text": true
        }
    },
    "attributes": {
        "domain": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "mhm-404-link",
    "editorScript": "file:./index.js",
    "viewScript": "file:./block.js",
    "style": "file:./style-index.css"
}
